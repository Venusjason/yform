{
  "npmClient": "npm",
  "command": {
    "publish": {
      "allowBranch": [
        "master"
      ],
      "npmClientArgs": ["--no-package-lock"]
    },
    "ignoreChanges": [
      "packages/.vuepress/**",
      "*.md",
      "**/package-lock.json",
      "**/example/**",
      "**/test/**",
      "**/dist/**",
      "**/lib/**"
    ]
  },
  "packages": [
    "packages/vue"
  ],
  "version": "independent"
}
